{
    "$schema": "http://json-schema.org/schema",
    "$id": "schema.json",
    "title": "Film",
    "description": "A film",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "last_update": {
            "type": "string",
            "format": "date-time"
        },
        "title": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "release_year": {
            "type": "integer"
        },
        "language_id": {
            "type": "integer"
        },
        "rental_duration": {
            "type": "integer"
        },
        "rental_rate": {
            "type": "number"
        }
    },
    "required": [ "id", "title", "language_id", "rental_duration", "rental_rate" ]
}